{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/todo",
      "status": 302,
      "headers": { "Location": "https://dida365.com/#q/all/today" }
    },
    {
      "src": "/now",
      "status": 302,
      "headers": { "Location": "https://vercel.com/dashboard" }
    },
    {
      "src": "/gist",
      "status": 302,
      "headers": {
        "Location": "https://gist.github.com/uniquexiaobai?direction=desc&sort=updated"
      }
    },
    {
      "src": "/npm",
      "status": 302,
      "headers": {
        "Location": "https://www.npmjs.com/settings/lokibai/packages"
      }
    },
    {
      "src": "/interview",
      "status": 302,
      "headers": { "Location": "https://www.yuque.com/lokibai/interview" }
    },
    {
      "src": "/react",
      "status": 302,
      "headers": { "Location": "https://www.yuque.com/lokibai/react" }
    },
    {
      "src": "/wiki",
      "status": 302,
      "headers": { "Location": "https://www.yuque.com/lokibai/wiki" }
    }
  ],
  "rewrites": [
    {
      "source": "/sheets/hao",
      "destination": "https://sheets.googleapis.com/v4/spreadsheets/1nwgg4TWOH86MTcnu8L7T0NGoeAy_Tkd6NGZAfcNS_hA/values/main"
    }
  ]
}
