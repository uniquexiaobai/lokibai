{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@now/static"
    }
  ],
  "redirects": [
    {
      "source": "/todo",
      "destination": "https://dida365.com/#q/all/today"
    },
    {
      "source": "/now",
      "destination": "https://vercel.com/dashboard"
    },
    {
      "source": "/gist",
      "destination": "https://gist.github.com/uniqzexiaobai?direction=desc&sort=updated"
    },
    {
      "source": "/npm",
      "destination": "https://www.npmjs.com/settings/lokibai/packages"
    },
    {
      "source": "/interview",
      "destination": "https://www.yuque.com/lokibai/interview"
    },
    {
      "source": "/react",
      "destination": "https://www.yuque.com/lokibai/react"
    },
    {
      "source": "/wiki",
      "destination": "https://www.yuque.com/lokibai/wiki"
    }
  ],
  "rewrites": [
    {
      "source": "/sheets/hao",
      "destination": "https://sheets.googleapis.com/v4/spreadsheets/1nwgg4TWOH86MTcnu8L7T0NGoeAy_Tkd6NGZAfcNS_hA/values/main"
    }
  ]
}
