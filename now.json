{
	"version": 2,
	"builds": [
		{
			"src": "**",
			"use": "@now/static"
		}
	],
	"routes": [
		{
			"src": "/todo",
			"status": 302,
			"headers": { "Location": "https://dida365.com/#q/all/today" }
		},
		{
			"src": "/now",
			"status": 302,
			"headers": { "Location": "https://zeit.co/dashboard" }
		},
		{
			"src": "/gist",
